{
    "name": "tomdkd/excel-database-importer",
    "description": "Symfony bundle who can be able to generate a database from an excel or csv file",
    "autoload": {
        "psr-4": {
            "Tomdkd\\ExcelDatabaseImporter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Thomas Dominik",
            "email": "dominik.thomasdl@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/symfony": "^6.0",
        "phpoffice/phpspreadsheet": "^1.23"
    },
    "require-dev": {
        "cweiske/php-sqllint": "^0.2.3",
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "*"
    }
}
